{
  "name": "browser-game-loader",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/katai5plate/browser-game-loader",
  "author": "katai5plate <katai5plate.sub@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nw\\nw.exe",
    "setupNW": "run-s setupNW:*",
    "setupNW:clean-nw": "node scripts/cleanNwDir",
    "setupNW:delete-sdk": "rimraf \"./download/nw-sdk.zip\" \"./download/nw-sdk/*\"",
    "setupNW:download-sdk": "node scripts/dl \"https://dl.nwjs.io/v0.51.2/nwjs-sdk-v0.51.2-win-x64.zip\" \"nw-sdk.zip\"",
    "setupNW:unzip-sdk": "rimraf \"./download/nw-sdk/\" && node scripts/unzip \"nw-sdk.zip\"",
    "setupNW:optimize-sdk": "rimraf \"./download/nw-sdk/{index.html,package.json}\"",
    "setupNW:move-sdk": "cpx \"./download/nw-sdk/**/*\" \"./nw/\""
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "cpx": "^1.5.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "progress": "^2.0.3",
    "rimraf": "^3.0.2"
  }
}
